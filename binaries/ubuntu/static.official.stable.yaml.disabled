#!/SBUILD ver @v1.0.0
#SELF: https://raw.githubusercontent.com/pkgforge/soarpkgs/refs/heads/main/binaries/ubuntu.yaml
_disabled: false

pkg: "ubuntu"
pkg_id: "github.com.docker-library.docs.tree.master.ubuntu"
pkg_type: "static"
category:
  - "ConsoleOnly"
  - "Utility"
description: "Portable Ephemeral Ubuntu Linux Docker Image (DockerC)"
homepage:
  - "https://github.com/docker-library/docs/tree/master/ubuntu"
maintainer:
  - "Azathothas (https://github.com/Azathothas)"
note:
  - "[DO NOT RUN] (Meant for pkgforge CI Only)"
  - "Created using DockerC, might require ROOT (Details: https://hub.docker.com/_/ubuntu)"
repology:
  - "ubuntu"
src_url:
  - "https://github.com/docker-library/docs/tree/master/ubuntu"
tag:
  - "Utility"
x_exec:
  host:
    - "aarch64-Linux"
    - "x86_64-Linux"
  shell: "bash"
  pkgver: |
    {EMPTY}
  run: |
    #!/usr/bin/env bash

#-------------------------------------------------------#
#Sanity Checks
if [ "${BUILD}" != "YES" ] || \
   [ -z "${BINDIR}" ] || \
   [ -z "${EGET_EXCLUDE}" ] || \
   [ -z "${EGET_TIMEOUT}" ] || \
   [ -z "${GIT_TERMINAL_PROMPT}" ] || \
   [ -z "${GIT_ASKPASS}" ] || \
   [ -z "${GITHUB_TOKEN}" ] || \
   [ -z "${SYSTMP}" ] || \
   [ -z "${TMPDIRS}" ]; then
 #exit
  echo -e "\n[+]Skipping Builds...\n"
  exit 1
fi
#-------------------------------------------------------#

#-------------------------------------------------------#
##Main
export SKIP_BUILD="NO" #YES, in case of deleted repos, broken builds etc
if [ "${SKIP_BUILD}" == "NO" ]; then
   #ubuntu: Portable Ephemeral ubuntu Linux Docker Image (DockerC)
     export BIN="ubuntu"
     export SOURCE_URL="https://github.com/docker-library/docs/tree/master/ubuntu"
     echo -e "\n\n [+] (Building | Fetching) ${BIN} :: ${SOURCE_URL} [$(TZ='UTC' date +'%A, %Y-%m-%d (%I:%M:%S %p)') UTC]\n"
      #Build
       sudo dockerc --image "docker://ubuntu:latest" --output "$BINDIR/ubuntu.no_strip"
       sudo chown -R "$(whoami):$(whoami)" "$BINDIR/ubuntu.no_strip" && chmod -R 755 "$BINDIR/ubuntu.no_strip"
       file "$BINDIR/ubuntu.no_strip" && du -sh "$BINDIR/ubuntu.no_strip"
fi
#-------------------------------------------------------#

#-------------------------------------------------------#
##Cleanup
unset SKIP_BUILD ; export BUILT="YES"
#In case of zig polluted env
unset AR CC CFLAGS CXX CPPFLAGS CXXFLAGS DLLTOOL HOST_CC HOST_CXX LDFLAGS LIBS OBJCOPY RANLIB
#In case of go polluted env
unset GOARCH GOOS CGO_ENABLED CGO_CFLAGS
#PKG Config
unset PKG_CONFIG_PATH PKG_CONFIG_LIBDIR PKG_CONFIG_SYSROOT_DIR PKG_CONFIG_SYSTEM_INCLUDE_PATH PKG_CONFIG_SYSTEM_LIBRARY_PATH
#-------------------------------------------------------#
